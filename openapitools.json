{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.4.0",
    "generators": {
      "angularClient": {
        "generatorName": "typescript-angular",
        "output": "./generated-api",
        "inputSpec": "./swagger.json",
        "glob": "./*.{json,yaml,yml}",
        "additionalProperties": {
          "withInterfaces": true,
          "providedInRoot": "true",
          "enumPropertyNaming": "original",
          "enableIvy": "true",
          "dateLibrary": "dayjs",
          "stringEnums": "true"
        }
      }
    }
  }
}
